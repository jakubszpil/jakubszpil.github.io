{
  "name": "jakubszpil.github.io",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "packageManager": "npm@10.8.3",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "serve dist/client",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:ci": "vitest run --hideSkippedTests --silent --no-color",
    "deploy": "npm run typecheck && npm run test && npm run build",
    "clean-install": "rm -rf node_modules package-lock.json && npm install"
  },
  "dependencies": {
    "@headlessui/react": "2.2.9",
    "@packages/feature-articles": "*",
    "@packages/feature-courses": "*",
    "@packages/feature-projects": "*",
    "@packages/shared": "*",
    "@react-router/dev": "7.10.1",
    "@react-router/node": "7.10.1",
    "isbot": "5.1.32",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-router": "7.10.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "4.1.17",
    "@tailwindcss/typography": "0.5.19",
    "@tailwindcss/vite": "4.1.17",
    "@testing-library/react": "16.3.0",
    "@types/node": "24.10.1",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@types/uglify-js": "3.17.5",
    "@vitejs/plugin-react": "5.1.1",
    "babel-plugin-react-compiler": "1.0.0",
    "happy-dom": "20.0.11",
    "postcss": "8.5.6",
    "tailwindcss": "4.1.17",
    "tailwindcss-animate": "1.0.7",
    "turbo": "2.6.3",
    "typescript": "5.9.3",
    "uglify-js": "3.19.3",
    "vite": "npm:rolldown-vite@7.2.10",
    "vite-plugin-babel": "1.3.2",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "4.0.15"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.10"
  }
}
