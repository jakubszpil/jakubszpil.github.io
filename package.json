{
  "name": "jakubszpil.github.io",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "packageManager": "npm@10.8.3",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "serve dist/client",
    "typecheck": "turbo typecheck && tsgo --noEmit",
    "test": "turbo test && vitest run",
    "test:ci": "turbo test:ci && vitest run --hideSkippedTests --silent --no-color",
    "deploy": "npm run typecheck && npm run test && npm run build",
    "clean-install": "rm -rf node_modules package-lock.json && npm install"
  },
  "dependencies": {
    "@headlessui/react": "2.2.9",
    "@packages/feature-projects": "*",
    "@packages/shared": "*",
    "@react-router/dev": "7.12.0",
    "@react-router/node": "7.12.0",
    "isbot": "5.1.32",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-router": "7.12.0",
    "@radix-ui/react-dropdown-menu": "2.1.16",
    "@radix-ui/react-slot": "1.2.4",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "tailwind-merge": "3.4.0",
    "tiny-invariant": "1.3.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "4.1.18",
    "@tailwindcss/typography": "0.5.19",
    "@tailwindcss/vite": "4.1.18",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.2",
    "@types/node": "25.0.9",
    "@types/react": "19.2.8",
    "@types/react-dom": "19.2.3",
    "@types/uglify-js": "3.17.5",
    "@typescript/native-preview": "7.0.0-dev.20260115.1",
    "@vitejs/plugin-react": "5.1.2",
    "babel-plugin-react-compiler": "1.0.0",
    "happy-dom": "20.3.3",
    "postcss": "8.5.6",
    "tailwindcss": "4.1.18",
    "tailwindcss-animate": "1.0.7",
    "turbo": "2.7.5",
    "typescript": "5.9.3",
    "uglify-js": "3.19.3",
    "vite": "npm:rolldown-vite@7.3.1",
    "vite-plugin-babel": "1.4.1",
    "vite-tsconfig-paths": "6.0.4",
    "vitest": "4.0.17",
