{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "!.react-router/**"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "start": {
      "persistent": true,
      "cache": false,
      "dependsOn": ["check-types", "build"]
    },
    "test": {},
    "test:ci": {
      "dependsOn": ["^build"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    }
  }
}
